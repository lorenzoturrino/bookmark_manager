<h1> Hello <%= @username %>, your id is <%= @user_id %> </h1>
<h2> <%= @email %> </h2>

<h2>Links</h2>

<ul id="links">
  <% @links.each do |link| %>
    <li>
      Title: <a href="<%= link.url %>"><%= link.title %></a>
      Tag: <%= link.tags.map(&:name).join(', ') %>
    </li>
  <% end %>
</ul>

<p> <%= @number_of_users %> user </p>
